@startuml
class 空車リスト{
- 車番号:String   
}
class 車{
- 車番号:String
- 車種:String 
- 駆動方式:String  
- 貸出料金:int 
}
class 顧客{
- 顧客ID:String  
- 氏名:String
- 電話番号:String   
- 郵便番号:String   
- 住所:String 
---
+ setName(氏名:String) void  
}
class 予約{
- 予約番号:int 
- 車番号:String
- 顧客ID:String
- 予約日:String  
}
class 貸出{
- 貸出番号:int 
- 車番号:String
- 顧客ID:String
- 予約日:String    
}
@enduml